<view class='listobj list' wx:for="{{list}}" wx:key="{{item.objectId}}" data-type='zhanxun' data-id='{{item.objectId}}' bindtap="click">
    <image mode='aspectFill' src='{{item.img}}'></image>
    <view>
      <text>{{item.title}}</text>
      <text style='float:right;margin-right:1vw;'>{{item.price}}</text>
    </view>
    <view class='tdp'>
      <text class='fontmin'>{{item.subTitle}}</text>
      <view class='purchase fontmin' >购票</view>
      <!--<view class='purchase fontmin' wx:if="{{item.feeDesc!='免费'}}">购票</view>-->
    </view>
    <!--<view style='margin:2vw 0 4vw 0;background: {{index!=list.length-1?"#dadada":"#fff"}};' class='xline'></view>-->
</view>