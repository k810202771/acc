page{
  font-size: 12px;
  font-weight: 400;
  position: relative;
  width: 100%;
  height: 100%;
}
.c-scroll{
  position: absolute;
  right: 10px;
  top: 0;
  bottom: 0;
  left: 10px;
  overflow: auto;
}


.title{
  height: 48px;
  line-height: 48px;
  text-align: center;
  font-size: 16px;
  font-weight: 500;
  margin: 10px 0;
}

.left-line{
  vertical-align: 10%;
  margin-right: 10px;
  display: inline-block;
  height: 2px;
  width: 30px;
  background: #fbab24;
}

.right-line{
  vertical-align: 10%;
  margin-left: 10px;
  display: inline-block;
  height: 2px;
  width: 30px;
  background: #fbab24;
}
