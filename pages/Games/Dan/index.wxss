.exchange{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 9999;
}
.exchangeWindow{
  position: absolute;
  width: 200px;
  height: 240px;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  margin: auto;
  overflow: visible;
  color: #3d3d3d;
  animation: windowOpen 0.3s;
}
.ex-Close{
  position: absolute;
  right: 0;
  top: 0;
  transform: translate3d(50%,-50%,0);
  width: 32px;
}
.ex-title{
  margin-top: 10px;
  height: 32px;
  line-height: 32px;
  font-size: 16px;
  display: block;
  text-align: center;
}
.ex-text{
  font-size: 12px;
  display: block;
  text-align: center;
}
.ex-icontxt{
  display: inline-block;
  width: auto;
  height: 40px;
  overflow: visible;
  position: absolute;
  top: 80px;
  left: 50%;
  transform: translateX(-50%);
}
.ex-it-icon{
  padding: 0;
  margin: 0;
  height: 100%;
  width: 40px;
  display: block;
  float: left;
}
.ex-it-text{
  font-size: 16px;
  float: left;
  display: block;
  margin: auto;
  height: 40px;
  line-height: 40px;
  padding-right: 20px;
}
.ex-integral{
  position: absolute;
  top: 140px;
  left: 50%;
  transform: translateX(-50%);
  width: 30%;
  background: #7f7f7f;
  border-radius: 90px;
  height: 16px;
  padding: 2px;
}
.ex-i-icon{
  width: 16px;
  height: 16px;
  float: left;
}
.ex-i-text{
  float: left;
  font-size: 12px;
  height: 16px;
  line-height: 16px;
  overflow: visible;
  width: calc(100% - 16px);
  text-align: center;
  color: #fff;
}
.ex-button{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 20px;
  height: 34px;
  width: 60%;
  line-height: 34px;
  border-radius: 5px;
  text-align: center;
  background: #ffcc35;
  color: #fff;
}
@keyframes k-play{
  0% {
      transform: scale3d(0,0,1);
  }
  40%{ 
      transform: scale3d(0,0,1);
  }
  70%{
      transform: scale3d(2,2,1);
  }
  100% {
      transform: scale3d(1,1,1);
  }
}