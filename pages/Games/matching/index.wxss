page{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.Uibox{
  width: 100%;
  height: 100%;
  position: relative;
  overflow: hidden;
}
.looking{
    position:absolute;
    left:0;
    top: 0;
    bottom: 0;
    right: 0;
    margin: auto;
}
@keyframes k-loadingO{
  0% {
      transform: rotateZ(0deg);
  }
  100% {
      transform: rotateZ(360deg);
  }
}
@keyframes k-loading1{
  0% {
      transform: scale3d(1,1,1);
  }
  100% {
      transform: scale3d(0,0,1);
  }
}
.left{
  position: absolute;
  background: #65d4ef;
  width: 100%;
  height: 100%;
  transform-origin:50%;
  transition: transform 0.5s;
}
.right{
  position: absolute;
  background: #fe7273;
  width: 100%;
  height: 100%;
  transform-origin:50%;
  transition: transform 0.5s;
  top: 100%;
}
.content{
  position: absolute;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.leftUser{
  position: absolute;
  left: 0;
  top: 50px;
  width: 250px;
  height: 100px;
  border-radius: 0 90px 90px 0;
  background: #fff;
  transition: transform 0.5s;
}
.rightUser{
  position: absolute;
  right: 0;
  bottom: 50px;
  width: 250px;
  height: 100px;
  border-radius: 90px 0 0 90px;
  background: #fff;
  transition: transform 0.5s;
}
.UserIcon{
  border-radius: 5px;
  position: absolute;
  top: 20px;
  left: 10px;
  height: 60px;
  width: 60px;
}
.UserIconRight{
  left: 40px;
}
.UserText{
  color: #565555;
  position: absolute;
  top: 50%;
  left: 80px;
  right: 10px;
  transform: translateY(-50%);
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.UserTextRight{
  left: 110px;
}
.VSicon{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  transition: transform 0.5s;
}