page{
  width: 100%;
  height: 100%;
  background: #0d2872;
}
.svg{
  position: absolute;
  top: 50vw;
  width: 100vw;
  height: 100vw;
  z-index: 99;
  overflow: visible;
}
.header{
  width:100vw;
  height: 53.24vw;
}
.svg_bog{
  position: absolute;
  transform: translate3d(-50%,-50%,0);
  overflow: visible;
  text-align: center;
  line-height: 0.7rem;
  z-index: -1;
}
.svg_img{
  width: 10vw;
  height: 10vw;
}
.svg_text{
  font-size: 3vw;
  color: #fff;
}
.title{
  position: absolute;
  width: 50vw;
  height: 26vw;
  z-index: 99;
  top: 2vw;
  left: 10vw;
  font-size: 3.5vw;
  line-height: 5vw;
  padding: 2vw;
  box-sizing: border-box;
  overflow: visible;
  color: #131d39;
}

.name{
  font-weight: bold;
  font-size: 4vw;
  text-align: center;
  overflow: visible;
}
.bg{
  position: absolute;
  top: 50vw;
  left: 0;
  right: 0;
  width: 100vw;
  height: 116vw;
}
.fudong{
  position: fixed;
  z-index: -99;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .8);
}
.save{
  position: fixed;
  height: 135vw;
  width: 80vw;
  left: 0;
  right: 0;
  top: 0;
  bottom: 15vw;
  margin: auto;
  background: #fff;
  z-index: 99999;
}
.button{
  position: absolute;
  top: 150vw;
  left: 0;
  right: 0;
  margin: 5vw;
}
.button button{
  display: inline-block;
  width:45%;
  font-size: 4vw;
}
.button button + button{
  margin-left: 10%;
}
.muse{
  position: absolute;
  border-radius: 3px;
  left: 0;
  right: 0;
  top: 180vw;
  height: 34vw;
  background: #fff;
  margin: 5vw;
  margin-bottom: 15vw;
  overflow: visible;
}
.m_left{
  float: left;
}
.m_img{
  width: 20.6vw;
  height: 28vw;
  margin: 3vw;
}
.m_right{
  margin: 3vw;
}
.m_title{
  color: #f4ba27;
  font-size: 4.5vw;
  margin-bottom: 3vw;
}
.m_text{
  font-size: 4vw;
}
.m_view{
  font-size: 4vw;
}
.savebutton{
  position: fixed;
  bottom: 0;
  width: 80vw;
  margin: 0 10vw 5vw 10vw;
  font-size: 4vw;
  z-index: 999999;
}

.retest{
  position: absolute;
  top: 165vw;
  left: 0;
  right: 0;
  margin: 5vw;
}
.retest button{
  font-size: 4vw;
  width: 100%;
}